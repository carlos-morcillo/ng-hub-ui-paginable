		"@angular/router": "^14.0.4",
		"@fortawesome/fontawesome-free": "^5.15.2",
		"@ngx-translate/core": "^13.0.0",
		"@types/jasmine": "~3.6.0",
		"@types/node": "^12.11.1",
		"bootstrap": "^5.1.0",
		"codelyzer": "^0.0.28",
		"jasmine-core": "~3.6.0",
		"jasmine-spec-reporter": "~5.0.0",
		"karma": "^6.3.4",
